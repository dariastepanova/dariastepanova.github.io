chief(Y):-hasowner(X,Y),projfile(X).

% staff request is granted unless it is denied 
grant(X):-&cDL["instances/inst_size_070_inst_004.owl",p,b,c,d,"StaffRequest"](X), not deny(X).

% blacklisted staff request is always denied
deny(X):-&cDL["instances/inst_size_070_inst_004.owl",q,b,c,d,"BlacklistedStaffRequest"](X).

% owners of files must have access to them
