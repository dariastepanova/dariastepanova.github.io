deny(X):-&cDL["instances/inst_size_030_inst_014.owl",q,b,c,d,"BlacklistedStaffRequest"](X).

% owners of files must have access to them
:-hasowner(Y,Z), deny(X), &rDL["instances/inst_size_030_inst_014.owl",a,b,c,d,"hasTarget"](X,Y), &rDL["instances/inst_size_030_inst_014.owl",a,b,c,d,"hasSubject"](X,Z).

% update information for the first DL-atom
p("Project",X):-projfile(X).

